<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.course.model">
    <!--获取登录接口case-->
    <select id="loginCase" parameterType="Integer" resultType="com.course.model.LoginCase">

        select * from t_loginCase where id = #{id};
    </select>
    
    <!--添加用户接口case-->
    <select id="addUserCase" parameterType="Integer" resultType="com.course.model.AddUserCase">
        select * from t_addusercase where id = #{id};
    </select>

    <!--获取用户接口case-->
    <select id="getUserInfoCase" parameterType="Integer" resultType="com.course.model.GetUserInfoCase">
        select * from t_getuserinfocase where id = #{id};
    </select>

    <!--获取用户列表case-->
    <select id="getUserListCase" parameterType="Integer" resultType="com.course.model.GetUserListCase">
        select * from t_getuserlistcase where id = #{id};
    </select>

    <!--更新/删除用户信息case-->
    <select id="UpdateUserInfo" parameterType="Integer" resultType="com.course.model.UpdateUserInfoCase">
        select * from t_updateuserinfocase where id = #{id};
    </select>
</mapper>